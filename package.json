{
  "name": "@compass-nft/ts-boilerplate",
  "version": "1.0.0",
  "description": "A simple Typescript boilerplate",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "lint": "eslint ./src",
    "build": "tsc -b",
    "dev": "nodemon -r dotenv/config 'src/index.ts' --exec 'TS_NODE_FILES=true TS_NODE_TRANSPILE_ONLY=true ts-node' src/index.ts -e ts"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "eslint": "^8.8.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-plugin-import": "^2.25.4",
    "typescript": "^4.5.5",
    "dotenv": "^10.0.0",
    "nodemon": "^2.0.14"
  }
}
